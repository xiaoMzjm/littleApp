/* pages/movelist/movelist.wxss */
.todayPlan{
  width: 100%;height: 70rpx;padding-top: 20px;color:#131b21;
}
.todayPlan-blank{
  width: 50rpx;height: 50rpx;float: left;
}
.todayPlan-title{
  float: left;letter-spacing: 10rpx;font-weight: 500;font-size: 42rpx;
}
.todayPlan-num{
  margin-left: 20rpx;float: left;letter-spacing: 5rpx;font-size: 32rpx;margin-top: 10rpx;
}



.how-many-days{
  width: 100%;height: 50rpx;padding-top: 5rpx;color: #565656;
}
.how-many-days-blank{
  width: 50rpx;height: 50rpx;float: left;
}
.how-many-days-days{
  float: left;font-size: 21rpx;letter-spacing: 5rpx;
}
.how-many-days-moves{
  font-size: 21rpx;letter-spacing: 5rpx;
}



.progress-bar{
  width: 100%;height: 30rpx;padding-top: 5rpx;color: #565656;
}
.progress-bar-blank{
  width: 50rpx;height: 30rpx;float: left;
}
.progress-bar-out{
  position: relative;width: 650rpx;height: 10rpx;background-color: #adadad;margin-left: 50rpx;border-radius: 5em;
}
.progress-bar-in{ 
  position: absolute;width: 300rpx;height: 10rpx;background-color: #34b073;border-radius: 5em;
}



.move-list{
  width: 100%;height: 150rpx;padding-top: 40rpx;color: #565656;
}
.move-list-blank{
  width: 50rpx;height: 150rpx;float: left;
}
.move-list-image-view{
  width: 162rpx;height: 150rpx;float: left;position: relative;
}
.move-list-image{
  width: 162rpx;height: 115rpx;position:absolute;
}
.move-list-image-mask{
  background-color: black;opacity: 0.4;width: 162rpx;height:115rpx;position: absolute;
}
.move-list-image-mask-gou{
  width: 20rpx;height: 20rpx;position:absolute;left:130rpx;top:80rpx;
}
.move-list-content{
  width: 200rpx;height: 150rpx;float: left;padding-left: 20rpx;
}
.move-list-content-title{
  font-weight: 500;font-size: 20rpx;color: #000000;letter-spacing: 10rpx;
}
.move-list-content-num{
  font-size: 20rpx;margin-top: 20rpx;color: #565656;letter-spacing: 5rpx;
}
.move-list-conent-relax{
  font-size: 20rpx;color: #565656;letter-spacing: 5rpx;
}
.move-list-play{
  float: left;width: 270rpx;height: 150rpx;
}
.move-list-play-image{
  width: 30rpx;height: 30rpx;float: right;margin-right: 30rpx;margin-top: 50rpx;
}
.move-list-line{
  width: 650rpx; height: 1rpx; background-color: #dcdcdc;float: left;margin-left: 50rpx; 
}




.bottom-blank{
  width: 100%;height: 100rpx;
}




.begin{
  width: 100%;height: 100rpx;position:fixed;background-color: #34b073;bottom:0rpx;
}
.begin-text{
  width: 100%;text-align: center;height: 100rpx;position: absolute;color: #ffffff;font-weight: 600;letter-spacing: 27rpx;line-height: 100rpx;
}